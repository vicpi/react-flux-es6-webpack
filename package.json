{
  "name": "products",
  "version": "0.0.1",
  "description": "Example React Flux Browserify architecture",
  "main": "dist/app.js",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babelify": "^7.2.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "gulp-babel": "^6.1.1",
    "history": "^1.13.1",
    "jquery-browserify": "1.8.1",
    "keymirror": "~0.1.0",
    "lodash": "^3.10.1",
    "object-assign": "^1.0.0",
    "react": "^0.13.3",
    "react-router": "^1.0.2",
    "react-tools": "^0.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "expect": "^1.13.4",
    "gulp": "3.9.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-spawn-mocha": "^2.2.2",
    "gulp-util": "^3.0.7",
    "jest-cli": "^0.4.3",
    "jsdom": "^3.1.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "reactify": "^1.1.1",
    "uglify-js": "~2.4.15",
    "vinyl-source-stream": "latest",
    "watchify": "^2.1.1",
    "webpack": "^1.12.11"
  },
  "scripts": {
    "start": "watchify -o dist/bundle.js -v -d dist/app.js",
    "build": "npm run compile && npm run browserify ",
    "test": "npm run compile && npm run mocha",
    "quick-test": "npm run mocha",
    "mocha": "mocha --compilers .:babel-core/register --compilers .:tests/lib/compiler.js tests/main.js",
    "browserify": "browserify dist/app.js -t | uglifyjs -cm > dist/bundle.min.js",
    "compile": "./node_modules/.bin/babel --presets es2015,stage-0 -d dist/ js/"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "../node_modules/babel-jest",
    "rootDir": "./js"
  }
}
